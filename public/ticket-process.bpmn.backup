<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="TicketProcess" name="Proceso de Gestión de Tickets - Fluyez" isExecutable="true">
    
    <!-- INICIO -->
    <bpmn:startEvent id="StartEvent" name="Cliente necesita soporte">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Cliente crea ticket -->
    <bpmn:userTask id="CreateTicket" name="Cliente crea ticket">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Registrar ticket en BD -->
    <bpmn:serviceTask id="SaveTicket" name="Registrar ticket en BD">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Gateway: ¿Asignación automática? -->
    <bpmn:exclusiveGateway id="Gateway_Assignment" name="¿Asignación automática?">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4a</bpmn:outgoing>
      <bpmn:outgoing>Flow_4b</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Asignación manual por Admin/Supervisor -->
    <bpmn:userTask id="ManualAssignment" name="Admin/Supervisor asigna ticket">
      <bpmn:incoming>Flow_4a</bpmn:incoming>
      <bpmn:outgoing>Flow_5a</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Auto-asignación por Agente -->
    <bpmn:userTask id="SelfAssignment" name="Agente se auto-asigna ticket">
      <bpmn:incoming>Flow_4b</bpmn:incoming>
      <bpmn:outgoing>Flow_5b</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Actualizar estado a IN_PROGRESS -->
    <bpmn:serviceTask id="UpdateStatusInProgress" name="Cambiar estado a En Progreso">
      <bpmn:incoming>Flow_5a</bpmn:incoming>
      <bpmn:incoming>Flow_5b</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Agente analiza ticket -->
    <bpmn:userTask id="AnalyzeTicket" name="Agente analiza ticket">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Gateway: ¿Requiere más información? -->
    <bpmn:exclusiveGateway id="Gateway_MoreInfo" name="¿Requiere más info?">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8a</bpmn:outgoing>
      <bpmn:outgoing>Flow_8b</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Solicitar información adicional -->
    <bpmn:userTask id="RequestInfo" name="Solicitar más información al cliente">
      <bpmn:incoming>Flow_8a</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Enviar notificación de solicitud -->
    <bpmn:serviceTask id="SendRequestEmail" name="Enviar email de solicitud">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Cambiar estado a PENDING -->
    <bpmn:serviceTask id="UpdateStatusPending" name="Cambiar estado a Pendiente">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Cliente responde -->
    <bpmn:userTask id="CustomerResponds" name="Cliente responde con información">
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Volver a análisis -->
    <bpmn:serviceTask id="BackToAnalysis" name="Retomar análisis">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Agente crea respuesta -->
    <bpmn:userTask id="CreateResponse" name="Agente crea respuesta">
      <bpmn:incoming>Flow_8b</bpmn:incoming>
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Gateway: ¿Es nota interna? -->
    <bpmn:exclusiveGateway id="Gateway_Internal" name="¿Es nota interna?">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_15a</bpmn:outgoing>
      <bpmn:outgoing>Flow_15b</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Guardar nota interna -->
    <bpmn:serviceTask id="SaveInternalNote" name="Guardar nota interna">
      <bpmn:incoming>Flow_15a</bpmn:incoming>
      <bpmn:outgoing>Flow_16a</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Guardar respuesta pública -->
    <bpmn:serviceTask id="SavePublicResponse" name="Guardar respuesta pública">
      <bpmn:incoming>Flow_15b</bpmn:incoming>
      <bpmn:outgoing>Flow_16b</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Enviar notificación por email -->
    <bpmn:serviceTask id="SendEmailNotification" name="Enviar email al cliente">
      <bpmn:incoming>Flow_16b</bpmn:incoming>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Gateway de convergencia -->
    <bpmn:exclusiveGateway id="Gateway_Converge">
      <bpmn:incoming>Flow_16a</bpmn:incoming>
      <bpmn:incoming>Flow_17</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Gateway: ¿Problema resuelto? -->
    <bpmn:exclusiveGateway id="Gateway_Resolved" name="¿Problema resuelto?">
      <bpmn:incoming>Flow_18</bpmn:incoming>
      <bpmn:outgoing>Flow_19a</bpmn:outgoing>
      <bpmn:outgoing>Flow_19b</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Continuar trabajando -->
    <bpmn:serviceTask id="ContinueWork" name="Continuar trabajando en ticket">
      <bpmn:incoming>Flow_19a</bpmn:incoming>
      <bpmn:outgoing>Flow_20</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Marcar como resuelto -->
    <bpmn:userTask id="MarkResolved" name="Marcar como resuelto">
      <bpmn:incoming>Flow_19b</bpmn:incoming>
      <bpmn:outgoing>Flow_21</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Actualizar estado a RESOLVED -->
    <bpmn:serviceTask id="UpdateStatusResolved" name="Cambiar estado a Resuelto">
      <bpmn:incoming>Flow_21</bpmn:incoming>
      <bpmn:outgoing>Flow_22</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Notificar resolución -->
    <bpmn:serviceTask id="NotifyResolution" name="Notificar resolución al cliente">
      <bpmn:incoming>Flow_22</bpmn:incoming>
      <bpmn:outgoing>Flow_23</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Gateway: ¿Cliente confirma? -->
    <bpmn:exclusiveGateway id="Gateway_Confirm" name="¿Cliente confirma resolución?">
      <bpmn:incoming>Flow_23</bpmn:incoming>
      <bpmn:outgoing>Flow_24a</bpmn:outgoing>
      <bpmn:outgoing>Flow_24b</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Cliente no confirma -->
    <bpmn:userTask id="CustomerNotConfirm" name="Cliente reabre ticket">
      <bpmn:incoming>Flow_24a</bpmn:incoming>
      <bpmn:outgoing>Flow_25</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Cerrar ticket -->
    <bpmn:userTask id="CloseTicket" name="Cerrar ticket">
      <bpmn:incoming>Flow_24b</bpmn:incoming>
      <bpmn:outgoing>Flow_26</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Actualizar estado a CLOSED -->
    <bpmn:serviceTask id="UpdateStatusClosed" name="Cambiar estado a Cerrado">
      <bpmn:incoming>Flow_26</bpmn:incoming>
      <bpmn:outgoing>Flow_27</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Generar estadísticas -->
    <bpmn:serviceTask id="GenerateStats" name="Actualizar estadísticas">
      <bpmn:incoming>Flow_27</bpmn:incoming>
      <bpmn:outgoing>Flow_28</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- FIN -->
    <bpmn:endEvent id="EndEvent" name="Ticket cerrado">
      <bpmn:incoming>Flow_28</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- FLUJOS -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent" targetRef="CreateTicket" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="CreateTicket" targetRef="SaveTicket" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="SaveTicket" targetRef="Gateway_Assignment" />
    <bpmn:sequenceFlow id="Flow_4a" name="Manual" sourceRef="Gateway_Assignment" targetRef="ManualAssignment" />
    <bpmn:sequenceFlow id="Flow_4b" name="Auto-asignación" sourceRef="Gateway_Assignment" targetRef="SelfAssignment" />
    <bpmn:sequenceFlow id="Flow_5a" sourceRef="ManualAssignment" targetRef="UpdateStatusInProgress" />
    <bpmn:sequenceFlow id="Flow_5b" sourceRef="SelfAssignment" targetRef="UpdateStatusInProgress" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="UpdateStatusInProgress" targetRef="AnalyzeTicket" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="AnalyzeTicket" targetRef="Gateway_MoreInfo" />
    <bpmn:sequenceFlow id="Flow_8a" name="Sí" sourceRef="Gateway_MoreInfo" targetRef="RequestInfo" />
    <bpmn:sequenceFlow id="Flow_8b" name="No" sourceRef="Gateway_MoreInfo" targetRef="CreateResponse" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="RequestInfo" targetRef="SendRequestEmail" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="SendRequestEmail" targetRef="UpdateStatusPending" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="UpdateStatusPending" targetRef="CustomerResponds" />
    <bpmn:sequenceFlow id="Flow_12" sourceRef="CustomerResponds" targetRef="BackToAnalysis" />
    <bpmn:sequenceFlow id="Flow_13" sourceRef="BackToAnalysis" targetRef="CreateResponse" />
    <bpmn:sequenceFlow id="Flow_14" sourceRef="CreateResponse" targetRef="Gateway_Internal" />
    <bpmn:sequenceFlow id="Flow_15a" name="Sí" sourceRef="Gateway_Internal" targetRef="SaveInternalNote" />
    <bpmn:sequenceFlow id="Flow_15b" name="No" sourceRef="Gateway_Internal" targetRef="SavePublicResponse" />
    <bpmn:sequenceFlow id="Flow_16a" sourceRef="SaveInternalNote" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_16b" sourceRef="SavePublicResponse" targetRef="SendEmailNotification" />
    <bpmn:sequenceFlow id="Flow_17" sourceRef="SendEmailNotification" targetRef="Gateway_Converge" />
    <bpmn:sequenceFlow id="Flow_18" sourceRef="Gateway_Converge" targetRef="Gateway_Resolved" />
    <bpmn:sequenceFlow id="Flow_19a" name="No" sourceRef="Gateway_Resolved" targetRef="ContinueWork" />
    <bpmn:sequenceFlow id="Flow_19b" name="Sí" sourceRef="Gateway_Resolved" targetRef="MarkResolved" />
    <bpmn:sequenceFlow id="Flow_20" sourceRef="ContinueWork" targetRef="AnalyzeTicket" />
    <bpmn:sequenceFlow id="Flow_21" sourceRef="MarkResolved" targetRef="UpdateStatusResolved" />
    <bpmn:sequenceFlow id="Flow_22" sourceRef="UpdateStatusResolved" targetRef="NotifyResolution" />
    <bpmn:sequenceFlow id="Flow_23" sourceRef="NotifyResolution" targetRef="Gateway_Confirm" />
    <bpmn:sequenceFlow id="Flow_24a" name="No" sourceRef="Gateway_Confirm" targetRef="CustomerNotConfirm" />
    <bpmn:sequenceFlow id="Flow_24b" name="Sí" sourceRef="Gateway_Confirm" targetRef="CloseTicket" />
    <bpmn:sequenceFlow id="Flow_25" sourceRef="CustomerNotConfirm" targetRef="AnalyzeTicket" />
    <bpmn:sequenceFlow id="Flow_26" sourceRef="CloseTicket" targetRef="UpdateStatusClosed" />
    <bpmn:sequenceFlow id="Flow_27" sourceRef="UpdateStatusClosed" targetRef="GenerateStats" />
    <bpmn:sequenceFlow id="Flow_28" sourceRef="GenerateStats" targetRef="EndEvent" />
    
  </bpmn:process>
  
